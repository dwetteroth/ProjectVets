#content.row
  .medium-12
    .top-image{style:"background-image:url(http://vinelist.com/assets/cow2.jpg);height:400px;width:100%;background-size: cover;"}

.row{style:"background-color:#fff;padding:20px;margin-bottom:20px;min-height:600px;margin-top:-20px;"}
  .medium-12.columns
    .blog-post
      %h3{style:"margin-bottom:25px;"}
        By Name
      -pasa_count = 0
      - @final.each do |name|
        -curr_name = name.split(',')[0]
        -curr_link = name.split(',')[1]

        -if(pasa_count>0 && pasa_count<=21)

          -pasa_count= pasa_count+1
          =raw "<a style='margin-left:30px;' href='#{curr_link}' target='_blan'>#{curr_name}</a>"
          %br

        -elsif(curr_name=="PASA"&& pasa_count==0)
          -pasa_count= pasa_count+1
          =raw "<a style='margin-left:0px;' href='#{curr_link}' target='_blan'>#{curr_name}</a>"
          %br

        -else
          =raw "<a style='margin-left:0px;' href='#{curr_link}' target='_blan'>#{curr_name}</a>"
          %br

